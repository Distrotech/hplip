post_depend_cmd=su - -c "/etc/init.d/cups restart", su - -c "chmod 700 /usr/lib/cups/backend/hp"
hplip_remove_cmd=su - -c "urpme hplip task-printing-hp"
su_sudo=su
ppd_install=drv
udev_mode_fix=1
ppd_dir=/usr/share/cups/model/HP
fix_ppd_symlink=0
drv_dir=/usr/share/cups/drv/HP

